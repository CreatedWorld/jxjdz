package avatar.module.mahjong.replay;


import java.util.Map;

public class PlayInfo {
    private String userId;
    private String sit;
    private String name;
    private String isMaster;
    private String headIcon;
    private String sex;
    private String isReady;
    private String score;
    private String isBanker;
    private Map<String, String> handCards;

    private String getCard;
    private Map<String, String> pengGangCards;
    private Map<String, String> putCards;

    public String getUserId() {
        return userId;
    }

    public String getSit() {
        return sit;
    }

    public String getName() {
        return name;
    }

    public String getIsMaster() {
        return isMaster;
    }

    public String getHeadIcon() {
        return headIcon;
    }

    public String getSex() {
        return sex;
    }

    public String getIsReady() {
        return isReady;
    }

    public String getScore() {
        return score;
    }

    public String getIsBanker() {
        return isBanker;
    }

    public Map<String, String> getHandCards() {
        return handCards;
    }

    public String getGetCard() {
        return getCard;
    }


    public void setUserId(String userId) {
        this.userId = userId;
    }

    public void setSit(String sit) {
        this.sit = sit;
    }

    public void setName(String name) {
        this.name = name;
    }

    public void setIsMaster(String isMaster) {
        this.isMaster = isMaster;
    }

    public void setHeadIcon(String headIcon) {
        this.headIcon = headIcon;
    }

    public void setSex(String sex) {
        this.sex = sex;
    }

    public void setIsReady(String isReady) {
        this.isReady = isReady;
    }

    public void setScore(String score) {
        this.score = score;
    }

    public void setIsBanker(String isBanker) {
        this.isBanker = isBanker;
    }

    public void setHandCards(Map<String, String> handCards) {
        this.handCards = handCards;
    }

    public void setGetCard(String getCard) {
        this.getCard = getCard;
    }

    public Map<String, String> getPengGangCards() {
        return pengGangCards;
    }

    public void setPengGangCards(Map<String, String> pengGangCards) {
        this.pengGangCards = pengGangCards;
    }

    public Map<String, String> getPutCards() {
        return putCards;
    }

    public void setPutCards(Map<String, String> putCards) {
        this.putCards = putCards;
    }
}
