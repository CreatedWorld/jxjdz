using System.Collections;
using System.Collections.Generic;
using UnityEngine;

/// <summary>
/// 商场信息数据
/// </summary>
public class ShoppingScrollData{
    /// <summary>
    /// 标题数据
    /// </summary>
    private string title;
    /// <summary>
    /// 图片名
    /// </summary>
    private string imageName;

    public string Title
    {
        get
        {
            return title;
        }
    }

    public string ImageName
    {
        get
        {
            return imageName;
        }
    }

    public ShoppingScrollData(string title,string imageName)
    {
        this.title = title;
        this.imageName = imageName;
    }
}
